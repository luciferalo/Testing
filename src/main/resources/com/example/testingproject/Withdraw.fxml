<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testingproject.WithdrawController">
   <children>
      <Label layoutX="349.0" layoutY="14.0" text="Withdraw Page">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="87.0" layoutY="121.0" text="Please enter the amount you need to withdraw from your account">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="withdrawtxt" alignment="CENTER" layoutX="187.0" layoutY="179.0" prefHeight="46.0" prefWidth="526.0" promptText="0000,00" />
      <Label fx:id="approvedlabel" layoutX="84.0" layoutY="360.0" text="  " textFill="LIME">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="withdrawbtn" layoutX="293.0" layoutY="264.0" mnemonicParsing="false" onAction="#withdrawbtnclicked" text="Withdraw">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label fx:id="failedlabel" layoutX="85.0" layoutY="365.0" text="  " textFill="RED">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="Backbtn" layoutX="508.0" layoutY="264.0" mnemonicParsing="false" onAction="#Backbtnclicked" prefHeight="43.0" prefWidth="113.0" text="Back">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="Logoutbtn" layoutX="730.0" layoutY="24.0" mnemonicParsing="false" onAction="#Logoutbtnclicked" prefHeight="43.0" prefWidth="113.0" text="Logout">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
